public func parse_int_with_base(s string, base int) int;
public func parse_uint_with_base(s string, base int) uint;

public func parse_int(s string) int
{
    return parse_int_with_base(s, 0);
}
public func parse_uint(s string) uint
{
    return parse_uint_with_base(s, 0);
}

public func parse_float(s string) float;
