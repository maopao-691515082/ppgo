public func rand() float;

func rand_n_impl(n int) int;
public func rand_n(n int) int
{
    if (n <= 0)
    {
        throw("non-positive n");
    }
    return rand_n_impl(n);
}

func fast_rand_n_impl(n int) int;
public func fast_rand_n(n int) int
{
    if (n <= 0)
    {
        throw("non-positive n");
    }
    return fast_rand_n_impl(n);
}
